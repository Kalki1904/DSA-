#include<bits/stdc++.h>
using namespace std;

long long int square_root(int n){
    int start=0;
    int end = n;
    long long int mid=start+(end-start)/2;
    int ans =0;
    while(start<=end){
        long long int sq=mid*mid;
        if(sq==n){
            return mid;
        }
        else if(sq>n){
            end=mid-1;
        }
        else if (sq<n){
            ans=mid;
            start=mid+1;
        }
        mid=start+(end-start)/2;
    }
    return ans;
}

int main(){

    int num=0;
    cout << "Enter Number" << endl;
    cin >>num;



    cout << "Square root = " << square_root(num);
}
